{
    "name": "fysiofitapp",
    "version": "1.0.0",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "lint": "eslint --ext .jsx --ext .js .",
        "format": "prettier --write **/*.{json,md}",
        "prepare": "husky install"
    },
    "dependencies": {
        "expo": "~45.0.0",
        "expo-status-bar": "~1.3.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-native": "0.68.2",
        "react-native-web": "0.17.7"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "eslint": "^8.15.0",
        "eslint-config-universe": "^11.0.0",
        "eslint-plugin-import": "^2.26.0",
        "husky": "^8.0.1",
        "lint-staged": "^12.4.1",
        "prettier": "^2.6.2"
    },
    "private": true,
    "lint-staged": {
        "src/**/*.{json,css,md}": "prettier --write",
        "**/*.{ts,tsx,js,jsx}": "eslint --fix"
    }
}
